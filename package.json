{
	"name": "git-cheatsheet",
	"displayName": "Git Cheatsheet",
	"description": "Open a Git Cheatsheet directly in VS Code.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:open.git.cheatsheet"
	],
	"license": "MIT",
	"icon": "images/icon.png",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open.git.cheatsheet",
				"title": "Open Git Cheatsheet"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dzhavat/git-cheatsheet-inside-vs-code"
	},
	"publisher": "dzhavat",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.31.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
